{
  "name": "Monocyanide ColorScheme - Black",
  "type": "dark",
  "semanticHighlighting": true,
  "author": "github.com/centril",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#f8f8f2",
    "editor.lineHighlightBackground": "#333333",
    "editor.selectionBackground": "#444444",
    "editor.selectionHighlightBackground": "#1c1c1c",
    "editorCursor.foreground": "#f8f8f0",
    "editorWhitespace.foreground": "#363636",
    "editorIndentGuide.background": "#635f2d",
    "editorIndentGuide.activeBackground": "#7fb11b",
    "editorGutter.background": "#073642",
    "editorGutter.modifiedBackground": "#967EFB",
    "editorGutter.addedBackground": "#aee837",
    "editorGutter.deletedBackground": "#ff4083",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#f8f8f2",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.inactiveForeground": "#696969",
    "activityBar.background": "#000000",
    "activityBar.foreground": "#f8f8f2",
    "activityBarBadge.background": "#ff4083",
    "activityBarBadge.foreground": "#f8f8f0",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#f8f8f2",
    "sideBarTitle.foreground": "#f8f8f2",
    "sideBarSectionHeader.background": "#333333",
    "sideBarSectionHeader.foreground": "#f8f8f2",
    "statusBar.background": "#000000",
    "statusBar.foreground": "#f8f8f2",
    "statusBar.noFolderBackground": "#000000",
    "statusBar.debuggingBackground": "#ff4083",
    "statusBar.debuggingForeground": "#f8f8f0",
    "terminal.background": "#000000",
    "terminal.foreground": "#f8f8f2",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#66d9ef",
    "terminal.ansiBrightBlack": "#696969",
    "terminal.ansiBrightBlue": "#66d9ef",
    "terminal.ansiBrightCyan": "#66d9ef",
    "terminal.ansiBrightGreen": "#aee837",
    "terminal.ansiBrightMagenta": "#ff4083",
    "terminal.ansiBrightRed": "#ff4083",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#ffa940",
    "terminal.ansiCyan": "#66d9ef",
    "terminal.ansiGreen": "#aee837",
    "terminal.ansiMagenta": "#be84ff",
    "terminal.ansiRed": "#ff4083",
    "terminal.ansiWhite": "#f8f8f2",
    "terminal.ansiYellow": "#e6db74",
    "tab.activeBackground": "#000000",
    "tab.activeForeground": "#f8f8f2",
    "tab.inactiveBackground": "#000000",
    "tab.inactiveForeground": "#696969",
    "list.activeSelectionBackground": "#444444",
    "list.activeSelectionForeground": "#f8f8f2",
    "list.inactiveSelectionBackground": "#333333",
    "list.inactiveSelectionForeground": "#f8f8f2",
    "list.hoverBackground": "#333333",
    "list.hoverForeground": "#f8f8f2",
    "list.highlightForeground": "#aee837"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#696969"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#e6db74"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#be84ff"
      }
    },
    {
      "name": "Constant: Built-in",
      "scope": ["constant.language", "meta.preprocessor"],
      "settings": {
        "foreground": "#be84ff"
      }
    },
    {
      "name": "Constant: User-defined",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#be84ff"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable.language", "variable.other"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#ff4083"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#ff4083"
      }
    },
    {
      "name": "Storage type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#66d9ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class"],
      "settings": {
        "foreground": "#aee837",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#aee837",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#aee837",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function argument",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#ffa940",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff4083"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#aee837"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#66d9ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#66d9ef"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#66d9ef",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "String constant",
      "scope": ["string constant"],
      "settings": {
        "foreground": "#66d9ef"
      }
    },
    {
      "name": "String.regexp",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#f6aa11"
      }
    },
    {
      "name": "String variable",
      "scope": ["string variable"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Variable: punctuation",
      "scope": ["punctuation.definition.variable"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Entity",
      "scope": ["entity"],
      "settings": {
        "foreground": "#aee837",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML: Doctype/XML Processing",
      "scope": [
        "meta.tag.sgml.doctype.xml",
        "declaration.sgml.html declaration.doctype",
        "declaration.sgml.html declaration.doctype entity",
        "declaration.sgml.html declaration.doctype string",
        "declaration.xml-processing",
        "declaration.xml-processing entity",
        "declaration.xml-processing string",
        "doctype"
      ],
      "settings": {
        "foreground": "#c8cecc"
      }
    },
    {
      "name": "HTML: Tags punctuation",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "CSS: Property Name",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#66d9ef"
      }
    },
    {
      "name": "CSS: #Id",
      "scope": ["meta.selector.css entity.other.attribute-name.id", "entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#ffc140"
      }
    },
    {
      "name": "CSS: .class",
      "scope": ["meta.selector.css entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#aee837"
      }
    },
    {
      "name": "JS: Function Name",
      "scope": ["meta.function.js", "entity.name.function.js", "support.function.dom.js"],
      "settings": {
        "foreground": "#aee837"
      }
    },
    {
      "name": "JSON: Dict",
      "scope": ["punctuation.definition.dictionary.begin.json", "punctuation.definition.dictionary.end.json"],
      "settings": {
        "foreground": "#aee837",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#f8f8f0",
        "background": "#ff4083"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#f8f8f0",
        "background": "#ae81ff"
      }
    }
  ]
} 